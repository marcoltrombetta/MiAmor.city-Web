        font-size: 35px;
        color: #36A0FF;
    }
</style>
<!--=== Profile ===-->
<div class="profile container content">
    <div class="row">
      
            @Html.Action("CustomerCategoryPartial", new { CustId = ViewBag.CustId, ContentId = ViewBag.ContentId })
    
    </div><!--/end row-->
