

<script type="text/javascript">
    function onClickLoadPage(pageurl) {
        jQuery.ajax({
            url: pageurl,
            data: {CustId:'@ViewBag.CustId', PageNumber:1},
            success: function (data) {
                $('#pageContent').html(data);
            }
        });
    }

</script>
